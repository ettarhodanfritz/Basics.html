let mass1=78
let mass2=98
let mass3=92
let mass4=85
let height1=1.69
let height2=1.88
let height3=1.95
let height4=1.76

function johnBodyMass1(){
    let BMI=mass1/height1*height1
    console.log(BMI)
}
johnBodyMass1()

function johnBodyMass2(){
    let BMI=mass2/height2*height1
    console.log(BMI)
}
johnBodyMass2()

function markBodyMass1(){
    let BMI=mass3/height3*height3
    console.log(BMI)
}
markBodyMass1()

function markBodyMass2(){
    let BMI=mass4/height4*height4
    console.log(BMI)
}
markBodyMass2()
